{
  "name": "galton",
  "version": "1.2.4",
  "description": "lightweight isochrone server",
  "main": "src/index.js",
  "author": "Stepan Kuzmin <to.stepan.kuzmin@gmail.com> (stepankuzmin.ru)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urbica/galton.git"
  },
  "bugs": {
    "url": "https://github.com/urbica/galton/issues"
  },
  "scripts": {
    "docs": "documentation build src/* --format md --output docs/API.md",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint .",
    "preinstall": "npm prune",
    "prepublish": "npm run docs",
    "start": "node index.js",
    "test": "tape -r babel-register test/index.js"
  },
  "bin": {
    "galton": "./index.js"
  },
  "pre-commit": [
    "flow",
    "lint",
    "test"
  ],
  "homepage": "https://github.com/urbica/galton#readme",
  "osrm_release": "master",
  "dependencies": {
    "babel-plugin-syntax-async-functions": "6.8.0",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.9.0",
    "babel-runtime": "6.9.2",
    "concaveman": "1.1.0",
    "kcors": "2.1.1",
    "koa": "2.0.0",
    "koa-morgan": "1.0.1",
    "minimist": "1.2.0",
    "npid": "0.4.0",
    "osrm": "5.2.6",
    "turf-bezier": "3.0.12",
    "turf-destination": "3.0.12",
    "turf-featurecollection": "1.0.1",
    "turf-point-grid": "3.0.12"
  },
  "devDependencies": {
    "babel-eslint": "6.1.0",
    "babel-plugin-transform-flow-strip-types": "6.8.0",
    "documentation": "4.0.0-beta2",
    "eslint": "3.0.0",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "1.10.2",
    "flow-bin": "0.28.0",
    "pre-commit": "1.1.3",
    "supertest-koa-agent": "0.2.1",
    "tape": "4.6.0"
  }
}
